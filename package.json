{
  "name": "mobx-failable",
  "version": "0.0.3",
  "description": "Simplified error handling in MobX",
  "main": "index.js",
  "typings": "index",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project tsconfig.json",
    "clean": "rm -r lib || true",
    "build": "npm-run-all build:compile lint",
    "build:compile": "tsc",
    "build:copy": "ts-node scripts/copy.ts",
    "package": "npm-run-all clean build build:copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrbanDoor/mobx-failable.git"
  },
  "author": "Urbandoor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UrbanDoor/mobx-failable/issues"
  },
  "homepage": "https://github.com/UrbanDoor/mobx-failable#readme",
  "devDependencies": {
    "@types/fs-extra": "0.0.37",
    "@types/jest": "^19.2.2",
    "fs-extra": "^2.0.0",
    "jest": "^19.0.2",
    "npm-run-all": "4.0.2",
    "ts-jest": "^19.0.14",
    "ts-node": "1.3.0",
    "tslint": "5.1.0",
    "typescript": "2.3.2"
  },
  "dependencies": {
    "mobx": "3.1.0",
    "typescript-string-enums": "0.3.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
