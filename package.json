{
  "name": "mobx-failable",
  "version": "0.2.0",
  "description": "Simplified error handling in MobX",
  "main": "index.js",
  "typings": "index",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project tsconfig.json",
    "clean": "rm -r lib || true",
    "build": "npm-run-all build:compile lint",
    "build:compile": "tsc",
    "build:copy": "ts-node scripts/copy.ts",
    "package": "npm-run-all clean build build:copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrbanDoor/mobx-failable.git"
  },
  "author": "Urbandoor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UrbanDoor/mobx-failable/issues"
  },
  "homepage": "https://github.com/UrbanDoor/mobx-failable#readme",
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/jest": "^24.0.6",
    "fs-extra": "^7.0.1",
    "jest": "^24.1.0",
    "mobx": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.10.0",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "mobx": "^4.9.0"
  },
  "dependencies": {
    "typescript-string-enums": "^0.3.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/src/init-test.ts"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
